{
  "name": "jwt-typescript-project",
  "version": "1.0.0",
  "description": "Проект для изучения работы с JWT на TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "examples": "ts-node src/examples/runExamples.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "release": "commit-and-tag-version",
    "release:patch": "commit-and-tag-version --release-as patch",
    "release:minor": "commit-and-tag-version --release-as minor",
    "release:major": "commit-and-tag-version --release-as major",
    "prepare": "husky install"
  },
  "keywords": ["jwt", "typescript", "authentication", "tokens"],
  "author": "Your Name",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "commit-and-tag-version": "^9.0.0",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "@types/bcryptjs": "^2.4.0",
    "express": "^4.18.0",
    "@types/express": "^4.17.0",
    "dotenv": "^16.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0"
  }
}
